QWidget {
    background-color: #f4ecd8;
    color: #5b4636;
    font-family: "Georgia", serif;

}

QPushButton {
    background-color: #e2d3b3;
    color: #4a3b2a;
    border: 1px solid #a19277;
    padding: 4px;
    border-radius: 4px;
}

QTableWidget, QListWidget, QLineEdit {
    background-color: #f4ecd8;
    color: #4a3b2a;
    border: 1px solid #bda87e;
    border-radius: 8px;
}

